generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Report {
  id         Int      @id @default(autoincrement())
  x_coord Float
  y_coord Float
  desc       String
  name       String
  added_date DateTime
}

model Opinion {
  id         Int      @id @default(autoincrement())
  x_coord Float
  y_coord  Float
  name       String
  desc       String
  rating     Int
  added_date DateTime
}
